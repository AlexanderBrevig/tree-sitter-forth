{
  "name": "tree-sitter-forth",
  "version": "0.2.0",
  "description": "Forth grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": ["parser", "lexer"],
  "scripts": {
    "build": "tree-sitter generate --abi 15 && node-gyp build",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "author": "Alexander Brevig",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.25.10"
  },
  "tree-sitter": [
    {
      "scope": "source.forth",
      "file-types": ["fs", "forth", "fth", "f", "4th"]
    }
  ]
}
